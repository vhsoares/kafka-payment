{
    "env": {
        "node": true,
        "es2021": true
    },
    "extends": [
        "xo"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "new-cap": "off"
    }
}